  <script>
          
          
   a=15
   b=50
   c=35
   d=60
      
      
      function create() {
          var graph=document.getElementById("hi");
          var table=graph.getContext("2d");
          table.font="20px ariel";
          color=["red", "blue", "orange", "green", "purple"];
          years=[2001, 2001, 2001, 2001, 2001];
          value=[182.3, 200.9, 292.3, 300.4, 650.4];
          
          for (i=0; i < color.length; i++) {
              table.fillStyle=color[i];
              table.fillRect(10,a,value[i],50);
              table.fillStyle="black";
              table.fillText=("Number of undernourish people (millions)" + years[i] + " " + value[i], 15, a);
              a = a + d
              c = c + d
              b = b + d } 
          
          
      } create()
              
              
              
          
          
          
          
      
          
          
          
          
          
        
          
          
